{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ],
    "env": {
      "MY_ENV_VARIABLE": "value"
    },
    "build": {
      "env": {
        "BUILD_ENV_VARIABLE": "value"
      }
    }
  }
  